dat <- read.csv('data/peers-cleaned/alldata.csv')
dat$subject
dat$subject %>% table()
subids <- unique(dat$subject)
library(tidyverse)
part1 <- filter(dat, subject %in% subids[1:115])
part2 <- filter(dat, subject %in% subids[116:230])
write.csv(part1, 'data/peers-cleaned/alldata_part1.csv', row.names = F)
write.csv(part2, 'data/peers-cleaned/alldata_part2.csv', row.names = F)
